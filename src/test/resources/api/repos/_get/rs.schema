{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "id",
        "name",
        "full_name",
        "private",
        "owner",
    ],
    "properties": {
        "id": {
            "type": "integer",
        },
        "node_id": {
            "type": "string",
        },
        "name": {
            "type": "string",
        },
        "full_name": {
            "type": "string",
        },
        "private": {
            "type": "boolean",
        },
        "owner": {
            "type": "object",
            "required": [
                "login",
                "id",
            ],
            "properties": {
                "login": {
                    "type": "string",
                },
                "id": {
                    "type": "integer",
                },
            },
            "additionalProperties": true
        },
        "description": {
            "type": "string",
        }
       },
    "additionalProperties": true
}
